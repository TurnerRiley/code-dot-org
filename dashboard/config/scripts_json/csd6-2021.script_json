      "position": 10,
      "properties": {
        "description": "**Distribute:** Give each student a copy of the peer review guide. Students should spend 15 minutes reviewing the other group's project and filling out the peer review guide."
      },
      "seeding_key": {
        "activity_section.key": "78dd2f16-eb2f-4b90-8640-f69e31d4a711",
        "lesson_activity.key": "1bd25a0d-f218-46e3-abef-7d656aa3e685"
