    {
      "key": "e49c7cce-066c-4c7d-87bb-a05375e20645",
      "position": 2,
      "properties": {
        "name": "Build Your Screens"
      },
      "seeding_key": {
        "activity_section.key": "e49c7cce-066c-4c7d-87bb-a05375e20645",
