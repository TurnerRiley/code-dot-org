    {
      "key": "c4e4b2fa-a2a3-4ca1-9bcd-33e8c65c3fb4",
      "position": 3,
      "properties": {
        "name": "Create your project"
      },
      "seeding_key": {
        "activity_section.key": "c4e4b2fa-a2a3-4ca1-9bcd-33e8c65c3fb4",
