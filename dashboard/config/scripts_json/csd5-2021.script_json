      }
    },
    {
      "key": "c069fdc8-dca0-438d-82ea-1cd2511207ce",
      "position": 1,
      "properties": {
        "description": "[slide] As students enter, have the warm-up slide projected on the board which contains 3 different representations from a \"Best Class Pet\" survey."
      },
      "seeding_key": {
        "activity_section.key": "c069fdc8-dca0-438d-82ea-1cd2511207ce",
